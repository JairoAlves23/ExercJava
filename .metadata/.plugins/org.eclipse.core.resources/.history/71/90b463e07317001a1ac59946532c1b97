package softblue.innerlocal;

public class Main {

	public static void main(String[] args) {
				
		class Texto {
			private String t;
			
			public Texto(String t) {
				this.t = t;
			}
			
			public void imprimir() {
				System.out.println(t);
			}
			 
		
		}

		Texto txt = new Texto("Meu texto");

	}

}
